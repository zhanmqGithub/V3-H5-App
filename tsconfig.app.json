{
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "include": [
    "env.d.ts",
    "src/**/*",
    "src/**/*.vue",
    "src/**/*.ts",
    "auto-imports.d.ts",
    "components.d.ts",
    "./node_modules/@types/html5plus/index.d.ts",
    "src/types/html5plus-fix.d.ts"
  ],
  "exclude": ["src/**/__tests__/*"],
  "compilerOptions": {
    "typeRoots": ["./node_modules/@types", "./src/types"],
    "lib": ["dom", "esnext"],
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.app.tsbuildinfo",
    "paths": {
      "@/*": ["./src/*"]
    }
  }
}
